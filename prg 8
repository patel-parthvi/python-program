def comp_str(str1, str2):
    if str1==str2:
        return 0
    elif str1>str2:
        return 1
    else:
        return -1
    
str1 = str(input("Enter the String1: "))
str2 = str(input("Enter the String2: "))
result = comp_str(str1, str2)
if result == 0:
    print("0")
elif result>0:
    print("1")
elif result<0:
    print("-1")